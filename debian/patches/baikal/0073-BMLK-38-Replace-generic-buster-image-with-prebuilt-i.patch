From 2cccf9c6a9ce59106acd080ed54c6fd9cf57c097 Mon Sep 17 00:00:00 2001
From: "Konstantin Kirik (snegovick)" <snegovick@uprojects.org>
Date: Thu, 1 Jul 2021 21:24:41 +0300
Subject: [PATCH 073/106] BMLK-38 : Replace generic buster image with prebuilt
 image

---
 .gitlab-ci.yml | 3 +--
 1 file changed, 1 insertion(+), 2 deletions(-)

diff --git a/.gitlab-ci.yml b/.gitlab-ci.yml
index 1001a05c962a..3faa350ef45a 100644
--- a/.gitlab-ci.yml
+++ b/.gitlab-ci.yml
@@ -3,14 +3,13 @@ variables:
 
 .build-kernel:
   stage: build
-  image: debian:buster-20210511
+  image: registry.t-platforms.ru/containers/common-kernel-builder:0-0-1
   variables:
     DEFCONFIG: defconfig
     BOARD: bm
   tags:
     - x86_64
   script:
-    - apt-get update && apt-get -y install crossbuild-essential-arm64 flex bison bc kmod cpio python3 rsync libssl-dev libncurses-dev curl git
     - ARCH=arm64 make $DEFCONFIG
     - SKIP=$(git describe --tags --no-abbrev | wc -m)
     - GIT_TAG=$(git describe --tags)
-- 
